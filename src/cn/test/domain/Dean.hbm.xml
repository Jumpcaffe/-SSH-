<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
   <class name="cn.test.domain.Dean" table="dean">
     <id name="did" column="did">
       <generator class="native"/>
     </id>
      <property name="dnumber" column="dnumber" length="20"/>
      <property name="dname" column="dname" length="20"/>
      <property name="dsex" column="dsex"/>
      <property name="dcollege" column="dcollege" length="20"/>
      <property name="dpassword" column="dpassword" length="20"/>
   </class>
</hibernate-mapping>